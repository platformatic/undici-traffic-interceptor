  "repository": {
    "type": "git",
    "url": "git+https://github.com/platformatic/undici-trafficante-interceptor.git"
  },
  "author": "Platformatic Inc. <oss@platformatic.dev> (https://platformatic.dev)",
  "dependencies": {
    "@node-rs/xxhash": "^1.7.6",
