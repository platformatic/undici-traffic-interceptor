    "undici": "^7.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.9",
    "eslint": "^9.21.0",
    "fastify": "^5.2.1",
    "neostandard": "^0.12.1",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist",
    "README.md"
