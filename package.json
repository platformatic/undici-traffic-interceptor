{
  "name": "undici-trafficante-interceptor",
  "version": "1.0.0",
  "description": "An Undici interceptor that allows you to inspect and filter HTTP traffic based on request and response data",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "private": true,
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "test": "node --test --experimental-strip-types test/*.test.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist;",
    "check": "pnpm lint && pnpm typecheck && pnpm test"
